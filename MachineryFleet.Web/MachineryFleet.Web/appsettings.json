{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OAuthProviders": {
    "Facebook": {
      "Authority": "https://www.facebook.com/v7.0/dialog/oauth",
      "ClientId": "your actual facebook client id",
      "ClientSecret": "your actual facebook client secret",
      "Scope": "public_profile email"
    },
    "Google": {
      "Authority": "https://accounts.google.com/oauth2/auth",
      "ClientId": "your actual google client id",
      "ClientSecret": "your actual google client secret",
      "Scope": "profile email"
    },
    "Twitter": {
      "Authority": "https://api.twitter.com/oauth2/token",
      "ClientId": "your actual twitter client id",
      "ClientSecret": "your actual twitter client secret",
      "Scope": "profile email"
    },
    "Microsoft": {
      "Authority": "https://login.microsoftonline.com/common/oauth2/v2.0/authorize",
      "ClientId": "your actual microsoft client id",
      "ClientSecret": "your actual microsoft client secret",
      "Scope": "profile email"
    }
  },
  "OpenIdConnectProviders": {
    "Okta": {
      "Authority": "https://your-okta-domain.okta.com/oauth2/default",
      "ClientId": "your actual okta client id",
      "ClientSecret": "your actual okta client secret"
    },
    "Auth0": {
      "Authority": "https://your-auth0-domain.auth0.com/",
      "ClientId": "your actual auth0 client id",
      "ClientSecret": "your actual auth0 client secret"
    }
  },
  "HttpLoggingOptions": {
    // Your HttpLogging options
  },
  "UseHsts": true,
  "Jwt": {
    "Key": "your actual jwt key",
    "Issuer": "http://localhost:35427/",
    "Audience": "http://localhost:35427/",
    "RefreshIssuer": "http://localhost:35427/",
    "RefreshAudience": "http://localhost:35427/",
    "Authority": "http://localhost:35427/",
    "ExpireMinutes": 60,
    "RefreshExpireMinutes": 60,
    "RefreshKey": "your actual refresh key",
    "SecretKey": "your secret key"
  },
  "AllowedHosts": "*"
}
